{
	"$schema": "https://turbo.build/schema.json",
	"globalPassThroughEnv": [],
	"tasks": {
		"topo": {
			"dependsOn": ["^topo"]
		},
		"lint": {
			"dependsOn": ["^topo"]
		},
		"lint:fix": {
			"dependsOn": ["^topo"],
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/"]
		},
		"typecheck": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/tsbuildinfo/"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
